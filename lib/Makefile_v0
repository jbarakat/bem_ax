include ../Makefile.in
#include $(LAPACK_DIR)/make.inc

OBJS =
OBJS += gaussleg.o

all: libBIE

#==============================================================================
# executable
libBIE: $(OBJS) \
	$(LIBRARIES)
	$(LOADER) $(LOADOPTS) $(OBJS) \
	$(LIBRARIES) -o $@
	./$@

#==============================================================================
# dependencies
.cpp.o:
	$(COMPILE) -o $@ $<

#==============================================================================
# clean
clean:
	rm -f *.o libBIE


