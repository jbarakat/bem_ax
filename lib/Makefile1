include ../Makefile.in

#==============================================================================
# objects
OBJS =
OBJS += gauleg.o

#==============================================================================
# target
all lib : libBIE.a

#==============================================================================
# executable
libBIE.a : $(OBJS) 
	@echo OBJS = $(OBJS) \
	$(LIBRARIES) \
	$(LOADER) $(LOADOPTS) $(OBJS) \
	$(LIBRARIES) -o $@
	g++ gauleg.o
	#./$@

#==============================================================================
# dependencies
.cpp.o :
	$(COMPILE) -o $@ $<

#==============================================================================
# clean
clean:
	rm -f *.o libBIE.a


